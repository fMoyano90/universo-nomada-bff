{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "application-core/quotation/templates/**/*",
        "outDir": "dist/"
      },
      {
        "include": "assets/images/**/*",
        "outDir": "dist/"
      }
    ]
  }
}